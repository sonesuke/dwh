out:
  type: postgresql
  host: postgres
  user: {{ env.POSTGRES_USER }}
  password: {{ env.POSTGRES_PASSWORD }}
  database: postgres
  schema: salesforce
  table: account
  mode: merge
  merge_keys:
    - Id